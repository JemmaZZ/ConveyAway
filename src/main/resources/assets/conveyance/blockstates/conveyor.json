{
  "multipart": [
	{   "when": { "facing": "north" },
		"apply": { "model": "conveyance:block/conveyor" }
	},
	{   "when": { "facing": "east" },
		"apply": { "model": "conveyance:block/conveyor", "y": 90 }
	},
	{   "when": { "facing": "south" },
		"apply": { "model": "conveyance:block/conveyor", "y": 180 }
	},
	{   "when": { "facing": "west" },
		"apply": { "model": "conveyance:block/conveyor", "y": 270 }
	},
	{   "when": { "top": "true" },
		"apply": { "model": "conveyance:block/conveyor_pillars" }
	},
	{   "when": { "facing": "north", "back": "false" },
		"apply": { "model": "conveyance:block/conveyor_back" }
	},
	{   "when": { "facing": "east", "back": "false" },
		"apply": { "model": "conveyance:block/conveyor_back", "y": 90 }
	},
	{   "when": { "facing": "south", "back": "false" },
		"apply": { "model": "conveyance:block/conveyor_back", "y": 180 }
	},
	{   "when": { "facing": "west", "back": "false" },
		"apply": { "model": "conveyance:block/conveyor_back", "y": 270 }
	},
	{   "when": { "facing": "north", "front": "false" },
		"apply": { "model": "conveyance:block/conveyor_guard" }
	},
	{   "when": { "facing": "east", "front": "false" },
		"apply": { "model": "conveyance:block/conveyor_guard", "y": 90 }
	},
	{   "when": { "facing": "south", "front": "false" },
		"apply": { "model": "conveyance:block/conveyor_guard", "y": 180 }
	},
	{   "when": { "facing": "west", "front": "false" },
		"apply": { "model": "conveyance:block/conveyor_guard", "y": 270 }
	}
  ]
}