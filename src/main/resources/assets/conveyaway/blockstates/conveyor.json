	},
	{   "when": { "facing": "east", "back": "false" },
		"apply": { "model": "conveyaway:block/conveyor_back", "y": 90 }
	},
	{   "when": { "facing": "south", "back": "false" },
		"apply": { "model": "conveyaway:block/conveyor_back", "y": 180 }
	},
	{   "when": { "facing": "west", "back": "false" },
		"apply": { "model": "conveyaway:block/conveyor_back", "y": 270 }
	},
	{   "when": { "facing": "north", "front": "false" },
		"apply": { "model": "conveyaway:block/conveyor_front" }
	},
	{   "when": { "facing": "east", "front": "false" },
		"apply": { "model": "conveyaway:block/conveyor_front", "y": 90 }
	},
	{   "when": { "facing": "south", "front": "false" },
		"apply": { "model": "conveyaway:block/conveyor_front", "y": 180 }
	},
	{   "when": { "facing": "west", "front": "false" },
